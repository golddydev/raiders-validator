{
  "name": "raiders-validator",
  "version": "0.4.3",
  "description": "This is raiders validator",
  "engines": {
    "node": ">=18.18",
    "npm": ">=10.5"
  },
  "author": "Golddy Dev <firingdev@gmail.com> (http://github.com/golddydev/)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf ./dist && npx tsc && rm -rf ./dist/tests",
    "prettier": "concurrently \"npx prettier -w '**/*.{js,jsx,ts,tsx,json,.*}'\" \"aiken fmt\"",
    "lint": "concurrently \"npx prettier --check '**/*.{js,jsx,ts,tsx,json,.*}'\" \"aiken fmt --check\" \"npx eslint . --max-warnings=0\"",
    "test:aiken": "concurrently \"aiken check\"",
    "test": "vitest run"
  },
  "bin": {
    "raiders": "./bin/raiders"
  },
  "dependencies": {
    "@blockfrost/blockfrost-js": "^5.5.0",
    "@lucid-evolution/lucid": "^0.4.17",
    "@noble/hashes": "^1.4.0",
    "bip39": "^3.1.0",
    "commander": "^11.1.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "enquirer": "^2.4.1",
    "flatted": "^3.3.1",
    "lodash": "^4.17.21",
    "raiders-helper": "^0.3.2",
    "p-limit": "^5.0.0",
    "p-retry": "^6.2.0",
    "parse-duration": "^1.1.0",
    "pretty-ms": "^9.0.0",
    "ts-res": "^1.1.1"
  },
  "devDependencies": {
    "@types/blake2": "^4.0.4",
    "@types/lodash": "^4.17.0",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "app-root-path": "^3.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^2.8.8",
    "tsx": "^4.9.3",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
